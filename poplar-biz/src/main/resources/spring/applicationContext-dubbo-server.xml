	<dubbo:service interface="com.pinganfu.fip.fbs.facade.view.RiskTestFacade" ref="riskTestFacade"
				   path="/RiskTestFacadeRemote"/>

	<dubbo:service interface="com.pinganfu.fip.fbs.facade.view.ProfitCouponFacade" ref="profitCouponFacade"
				   path="/ProfitCouponFacadeRemote"/>

	<dubbo:service interface="com.pinganfu.fip.fbs.facade.view.SpeedFacade" ref="speedFacade"
				   path="/SpeedFacadeRemote"/>

	<dubbo:service interface="com.pinganfu.fip.fbs.facade.fixedInvest.FixedInvestFacade" ref="fixedInvestFacade"
				   path="/FixedInvestFacadeRemote"/>


</beans>
